def next(l: list[_T]) -> _T:
  return l.pop()

def print_when_called(f: Callable[_P, _T]) -> Callable[_P, _T]:
  # highlight-next-line
  def inner(*args: _P.args, **kwargs: _P.kwargs) -> _T:
    print("Function was called")
    # This will error
